//arithmetic operands

void main() {
    int x;
    int y = 1;
    int z = 2;
    int w = 5;
    int res;
    res = x + y - w + 2; //-2
    printi(res);
    res = 2 * w + z * 3; //16
    printi(res);
    res = 2 * (w + z) * 3; //42
    printi(res);
    res = 10 / 2 * 10; //50
    printi(res);
    res = 10 / (2 * 10); //0
    printi(res);
    res = w + 3 * 2 / 2 - 15 / 5 * 3 - 2; //-3
    printi(res);
    res = w + ((3 * 2) / 2) - ((15 / 5) * 3) - 2; //-3
    printi(res);
    res = w + 3 * 2 / (2 - 15 / 5 * 3 - 2); //5
    printi(res);
    res = (w + 3 * 2) / (2 - 15 / 5 * 3 - 2); //-1
    printi(res);
    res = (w + 3) * ((2 / 2) - (15 / (5 * 3)) - 2); //-16
    printi(res);
}