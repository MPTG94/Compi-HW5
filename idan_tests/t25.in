bool printb(bool x) {
    if (x) printi(1);
    if (not x) print("0");
    return x;
}

void main()
{
    int i = 0;
    int x = 5;
    int y = 2;
    int z;
    while(i < 3) {
        while(z < x * y) {
            z = z + y;
            printi(z);
