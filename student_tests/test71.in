bool Func(int x) {
	return x > 3;
}
void maxTestsTestAndOrWithFunc() {
	bool x = Func(3) and true;
	bool y = Func(5) or 5 < 4;
	if (not x) print("Your mind is a powerful thing. When you fill it with positive thoughts, your life will start to change. – Unknown");
	if (y) print("Start by doing what’s necessary; then do what’s possible; and suddenly you are doing the impossible. – Francis of Assisi");
}

void main() {
     maxTestsTestAndOrWithFunc();
}
